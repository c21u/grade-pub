    "@instructure/ui-buttons": "^10.3.0",
    "@instructure/ui-checkbox": "^10.5.0",
    "@instructure/ui-date-input": "^10.3.0",
    "@instructure/ui-flex": "^10.5.0",
    "@instructure/ui-heading": "^10.3.0",
    "@instructure/ui-icons": "^10.3.0",
    "@instructure/ui-link": "^10.3.0",
    "@instructure/ui-list": "^10.3.0",
    "@instructure/ui-modal": "^10.3.0",
    "@instructure/ui-popover": "^10.3.0",
    "@instructure/ui-responsive": "^10.3.0",
    "@instructure/ui-spinner": "^10.3.0",
    "@instructure/ui-table": "^10.3.0",
    "@instructure/ui-text": "^10.3.0",
    "@instructure/ui-view": "^10.3.0",
    "bunyan": "^1.8.15",
    "bunyan-express-serializer": "^1.0.0",
    "buzzapi": "^2.1.0",
    "canvas-lms-api": "^1.0.2",
    "dotenv": "^16.4.3",
    "ejs": "~3.1.9",
    "express": "~4.18.2",
    "express-jwt": "^8.4.1",
    "http-errors": "~2.0.0",
    "ims-lti": "^3.0.2",
    "jsonwebtoken": "^9.0.2",
    "jwt-decode": "^4.0.0",
    "p-map": "^7.0.1",
