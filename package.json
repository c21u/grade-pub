{
  "name": "gradepub",
  "version": "1.0.6",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "webpack --config webpack.prod.js",
    "build-prod": "yarn run build",
    "build-dev": "webpack --config webpack.dev.js",
    "dev:client": "webpack --watch --config webpack.dev.js",
    "dev:server": "nodemon ./bin/www",
    "lint": "precise-commits",
    "start": "node ./bin/www",
    "start-dev": "run-p dev:*",
    "test": "NODE_ENV=test node --experimental-vm-modules $(yarn bin jest)",
    "prepare": "husky"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint && yarn test"
    }
  },
  "babel": {
    "presets": [
      [
        "@babel/env",
        {
          "targets": "defaults"
        }
      ],
      "@babel/preset-react"
    ]
  },
  "dependencies": {
