    "npm-run-all": "^4.1.5",
    "precise-commits": "^1.0.2",
    "prettier": "^3.2.5",
    "supertest": "^6.3.4",
    "webpack": "^5.90.1",
    "webpack-cli": "^5.1.4",
    "webpack-merge": "^5.10.0"
